num_open_chats = 0
max_open_chats = 2

function increment_open_chats() {
  if (num_open_chats == max_open_chats) {
    return
  }
  else {
    num_open_chats += 1
  }
}

function create_conversation(user_id) {
  increment_open_chats()
  // console.log("test")
  $('#chat1').html("test")

  $.ajax({
    url: "conversations/create_conversation",
    type: "POST",
    dataType:"json",
    data: {user: user_id}
  });
}
